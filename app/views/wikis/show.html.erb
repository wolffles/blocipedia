<div>
  <h1> <%= @wiki_page.title %> </h1>
  <p> <%= markdown(@wiki_page.body) %> </p>
  <small> Created at <%= time_ago_in_words(@wiki_page.created_at) %> </small>
</div>
  <%= link_to "Edit", edit_wiki_path(@wiki_page), class: 'btn btn-success' %>
  <%= link_to "Delete Topic", @wiki_page, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this topic?' } %>
